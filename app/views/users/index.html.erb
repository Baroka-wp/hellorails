<div>
    <% @users.each do |user| %>
    <div class="user_info">
        <%= image_tag user.photo, size: "160x90", alt: "Edit Entry"%>
        <div>
            <h1><%= link_to user.name, user %></h1>
            <p>Number of posts : <%= user.postsCounter %></p>
        </div>
    </div>
    <% end %>
</div>